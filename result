>        23:     call   t4 <- DIM
>        24:     if     t3 # t4 goto 9_if_true
>        25:     goto   10_if_false
>        26: 9_if_true:
>        27:     &()    t5 <- _str_6
>        28:     param  0 <- t5
>        29:     call   WriteStr
>        30:     return 0
>        31:     goto   8
>        32: 10_if_false:
>        33: 8:
>        34:     param  1 <- 0
