>        96:     param  1 <- 2
>        97:     &()    t28 <- MatRes
>        98:     param  0 <- t28
>        99:     call   t29 <- DIM
>       100:     mul    t30 <- rowcnt, t29
>       101:     add    t31 <- t30, colcnt
>       102:     mul    t32 <- t31, 4
>       103:     &()    t33 <- MatRes
>       104:     param  0 <- t33
>       105:     call   t34 <- DOFS
>       106:     add    t35 <- t32, t34
>       107:     add    t36 <- t27, t35
>       108:     assign @t36 <- t26
>       109:     add    t37 <- colcnt, 1
>       110:     assign colcnt <- t37
>       111:     goto   31_while_cond
>       112: 30:
>       113:     add    t38 <- rowcnt, 1
>       114:     assign rowcnt <- t38
>       115:     goto   27_while_cond
>       116: 26:
>       117:     return 1
>     ]]
>   ]]
