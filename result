>   [[ procedure: booltest3
>     [[
>       [ %p1       <ptr(4) to <array 5 of <array 5 of <bool>>>>       ]
>       [ $t0       <ptr(4) to <array 5 of <bool>>>       ]
>       [ $t1       <int>       ]
>       [ $t10      <int>       ]
>       [ $t11      <int>       ]
>       [ $t12      <int>       ]
>       [ $t13      <int>       ]
>       [ $t2       <ptr(4) to <array 5 of <bool>>>       ]
>       [ $t3       <int>       ]
>       [ $t4       <int>       ]
>       [ $t5       <int>       ]
>       [ $t6       <int>       ]
>       [ $t7       <int>       ]
>       [ $t8       <int>       ]
>       [ $t9       <int>       ]
>     ]]
>     [[ booltest3
>         0:     &()    t0 <- BoolArr0
>         1:     mul    t1 <- 1, 1
>         2:     &()    t2 <- BoolArr0
>         3:     param  0 <- t2
>         4:     call   t3 <- DOFS
>         5:     add    t4 <- t1, t3
>         6:     add    t5 <- t0, t4
>         7:     param  1 <- 2
>         8:     param  0 <- p1
>         9:     call   t6 <- DIM
>        10:     mul    t7 <- i0, t6
>        11:     mul    t8 <- i1, i2
>        12:     add    t9 <- t7, t8
>        13:     mul    t10 <- t9, 1
>        14:     param  0 <- p1
>        15:     call   t11 <- DOFS
>        16:     add    t12 <- t10, t11
>        17:     add    t13 <- p1, t12
>        18:     assign @t13 <- @t5
>        19:     return 
>     ]]
>   ]]
> ]]
