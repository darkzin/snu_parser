>       [ $t20      <int>       ]
>       [ $t21      <int>       ]
>       [ $t22      <int>       ]
>       [ $t23      <int>       ]
>       [ $t24      <int>       ]
>       [ $t25      <int>       ]
>       [ $t26      <int>       ]
>       [ $t27      <ptr(4) to <array 5 of <array 5 of <int>>>>       ]
>       [ $t28      <ptr(4) to <array 5 of <array 5 of <int>>>>       ]
>       [ $t29      <int>       ]
>       [ $t3       <int>       ]
>       [ $t30      <int>       ]
>       [ $t31      <int>       ]
>       [ $t32      <int>       ]
>       [ $t33      <ptr(4) to <array 5 of <array 5 of <int>>>>       ]
>       [ $t34      <int>       ]
>       [ $t35      <int>       ]
>       [ $t36      <int>       ]
>       [ $t37      <int>       ]
>       [ $t38      <int>       ]
>       [ $t4       <int>       ]
>       [ $t5       <ptr(4) to <array 30 of <char>>>       ]
>       [ $t6       <int>       ]
>       [ $t7       <ptr(4) to <array 28 of <char>>>       ]
>       [ $t8       <int>       ]
>       [ $t9       <ptr(4) to <array 28 of <char>>>       ]
>     ]]
>     [[ MatSub
>         0:     assign rowcnt <- 0
>         1:     assign colcnt <- 0
>         2:     param  1 <- 1
>         3:     param  0 <- A
>         4:     call   t0 <- DIM
>         5:     param  1 <- 1
